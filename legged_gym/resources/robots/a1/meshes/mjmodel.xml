      </default>
    
    
      <default class="visual">
      <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>
    
    
      <default class="collision">
      <geom group="3"/>
          <default class="foot">
          <geom size="0.022" pos="-0.002 0 -0.213" priority="1" condim="6" friction="0.4 0.02 0.01"/>
          </default>     
